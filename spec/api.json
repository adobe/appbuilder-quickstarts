{
  "openapi": "3.0.0",
  "info": {
    "title": "{{LIB_NAME}} REST API requests",
    "version": "v1.0.0"
  },
  "servers": [
    {
      "url": "https://my-server.com/"
    }
  ],
  "components": {
    "schemas": {
    },
    "requestBodies": {
