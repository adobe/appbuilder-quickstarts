        "in": "header",
        "name": "Cache-Control",
        "required": false,
        "schema": {
          "type": "string",
          "default": "no-cache"
        }
      },
      "Content-Type": {
        "in": "header",
        "name": "Content-Type",
        "required": false,
        "schema": {
          "type": "string",
          "default": "application/json;charset=utf-8"
        }
      }
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    },
    {
      "BearerAuth": []
    }
  ],
  "paths": {
    "/something": {
      "parameters": [
        {
          "$ref": "#/components/parameters/Cache-Control"
        },
        {
          "$ref": "#/components/parameters/Content-Type"
        }
      ],
      "get": {
        "summary": "Get something",
        "operationId": "getSomething",
        "tags": ["mytag"],
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    }
